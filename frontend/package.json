    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "vite": "^5.1.4",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-handlebars": "^1.6.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
