    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "vite": "^5.2.10",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-handlebars": "^1.6.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.1.4"
